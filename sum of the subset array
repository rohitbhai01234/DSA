#include <stdio.h>
#include <math.h>
int main() {
    int n, i;
    printf("Enter size of array: ");
    scanf("%d", &n);
    int arr[n];
    printf("Enter elements: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    long long sum = 0;
    for (i = 0; i < n; i++) {
        sum += arr[i];
    }
    long long total = sum * (1LL << (n - 1)); 
    printf("Sum of all subsets = %lld\n", total);
    return 0;
}
